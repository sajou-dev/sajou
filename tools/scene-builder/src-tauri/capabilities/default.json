{
  "identifier": "default",
  "description": "Default permissions for sajou desktop app",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://*" },
        { "url": "https://*" },
        { "url": "http://*:*" },
        { "url": "https://*:*" }
      ]
    }
  ]
}
